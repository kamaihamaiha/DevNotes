<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextWithClear">
        <attr name="clearIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="RectView">
        <attr name="rect_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MyTitleBar">
        <attr name="title_bg_color" format="color" />
        <attr name="title_text_color" format="color" />
        <attr name="title_text" format="string" />
    </declare-styleable>

    <declare-styleable name="ExplainView">
        <attr name="max_collapse_lines" format="integer" />
        <attr name="text_size" format="integer" />
        <attr name="bg_color" format="color" />
        <attr name="text_color" format="color" />
    </declare-styleable>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetBackgroundColor" format="color" />
        <attr name="appWidgetTextColor" format="color" />
    </declare-styleable>

</resources>